array degree[8] : w32 -> w8 = symbolic
array i[8] : w32 -> w8 = symbolic
array powx[8] : w32 -> w8 = symbolic
array res[8] : w32 -> w8 = symbolic
array x[8] : w32 -> w8 = symbolic
(query [(FOLt (ReadLSB w64 0 i)
               (ReadLSB w64 0 degree))
         (FOLt N0:(FMul w64 (ReadLSB w64 0 powx)
                            N1:(ReadLSB w64 0 x))
               (FSub w64 (FAdd w64 (ReadLSB w64 0 res)
                                   N0)
                         N1))]
        false)
